<!DOCTYPE html>
<html>
  <head>
    <%- include "parts/default-assets.ect" %>
    <title>
      <%- (@title or 'OpenCubes').replace /(\r\n|\n|\r)/g, "" %>
    </title>
  </head>
  <body>
    <div id="wrapper">
      <% content 'navbar' %>
      <div class="main container" id="content">
        <div class="ui grid container">
          <div class="four wide column">
            <% content 'sidebar' %>
          </div>
          <div class="eight wide column">
            <% content %>
          </div>
        </div>
      </div>
      <%- include "part/login-dialog.ect" %>
      <footer><%- include "parts/footer.ect" %></footer>
    </div>
  </body>
</html>